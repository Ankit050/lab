
{
  "title": "Microbuoyancy measured by gauges",
  "about": "Interactive example",
  "model": {
    "url": "/imports/legacy-mw-content/converted/tutorial/microbuoyancy$0.json",
    "viewOptions": {
      "controlButtons": "play_reset",
      "modelTimeLabel": true
    }
  },
  "components": [
    {
      "type": "button",
      "id": "gravity-on",
      "text": "Gravity On",
      "action": "set({ 'gravitationalField' : false });"
    },
    {
      "type": "button",
      "id": "gravity-off",
      "text": "Gravity Off",
      "action": "set({ 'gravitationalField' : 8e-8 });"
    },
    {
      "type": "energyGraph",
      "id": "ke-chart",
      "dimensions": { "width": 500, "height": 296 },
      "options": { "ymin": -1000, "ymax": 5000 }
    }
  ],
  "layout": {
    "rightwide": [
      "ke-chart"
    ],
    "bottom": [
      "gravity-on",
      "gravity-off"
    ]
  }
}