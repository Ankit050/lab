{
  "title": "DNA Animations Example",
  "publicationStatus": "draft",
  "subtitle": "",
  "about": "",
  "fontScale": 0.8,
  "models": [
    {
      "id": "page3$0",
      "url": "imports/legacy-mw-content/converted/sam-activities/DNA-to-proteins/page3/page3$0.json",
      "viewOptions": {
        "controlButtons": "",
        "enableKeyboardHandlers": false
      },
      "modelOptions": {
        "DNA": "ATGCCAGGCGGCGAGAGCTTGCTAATTGGCTTATAG",
        "geneticEngineState": "translation:0"
      }
    }
  ],
  "outputs": [
    {
      "name": "progressRate",
      "value": [
        "return getSimulationProgressRate();"
      ],
      "unitName": "femtoseconds per actual second"
    }
  ],
  "components": [
    {
      "type": "pulldown",
      "id": "state-pulldown",
      "options": [
        {
          "text": "Jump to genetic engine state...",
          "disabled": true,
          "selected": true
        },
        {
          "text": "dna",
          "action": "set('geneticEngineState', 'dna');"
        },
        {
          "text": "transcription:0",
          "action": "set('geneticEngineState', 'transcription:0');"
        },
        {
          "text": "transcription-end",
          "action": "set('geneticEngineState', 'transcription-end');"
        },
        {
          "text": "translation:0",
          "action": "set('geneticEngineState', 'translation:0');"
        }
      ]
    },
    {
      "type": "button",
      "id": "transcribe",
      "text": "Transcribe",
      "action": "transcribe();"
    },
    {
      "type": "button",
      "id": "transcribe-step",
      "text": "Transcription Step",
      "action": "transcribeStep();"
    },
    {
      "type": "button",
      "id": "translate-step",
      "text": "Translation Step",
      "action": "translateStep();"
    },
    {
      "type": "button",
      "id": "translate",
      "text": "Translate",
      "action": "translate();"
    },
    {
      "type": "button",
      "id": "reset",
      "text": "Reset",
      "action": "reset();"
    },
    {
      "id": "current-state",
      "type": "numericOutput",
      "property": "geneticEngineState",
      "label": "Current 'geneticEngineState'",
      "displayValue": "return value"
    },
    {
      "id": "viewport",
      "type": "slider",
      "title": "Viewport Position",
      "property": "viewPortX",
      "width": "35em",
      "displayValue": "return format('1.1f')(value)",
      "min": -31,
      "max": 35,
      "steps": 2000
    }
  ],
  "layout": {
    "bottom": [
      ["viewport"],
      ["state-pulldown", "reset"],
      ["current-state"]
    ],
    "right": [
      ["transcribe-step"],
      ["transcribe"],
      ["translate-step"],
      ["translate"]
    ]
  }
}