{
  "title": "Table, Graph and DataSet Demo",
  "publicationStatus": "public",
  "subtitle": "",
  "aspectRatio": 0.8,
  "models": [
    {
      "type": "md2d",
      "id": "simple-gas$0",
      "url": "imports/legacy-mw-content/converted/new-examples-for-nextgen/simple-gas$0.json",
      "viewOptions": {
        "controlButtons": "play_reset"
      }
    }
  ],
  "dataSets": [
    {
      "name": "table-dataset",
      "streamDataFromModel": false,
      "clearOnModelReset": false,
      "properties": [
        "time",
        "Editable col"
      ],
      "initialData": {
        "time": [0, 0],
        "Editable col": ["abc", "xyz"]
      }
    },
    {
      "name": "graph-dataset",
      "properties": [
        "displayTime",
        "totalEnergy"
      ],
      "clearOnModelReset": false
    }
  ],
  "components": [
    {
      "type": "text",
      "id": "label1",
      "text": "Left table is editable, right not. Both use the same data set, so they are synchronized:"
    },
    {
      "id": "table1",
      "type": "table",
      "dataSet": "table-dataset",
      "title": null,
      "addNewRows": true,
      "showBlankRow": true,
      "indexColumn": true,
      "height": "10em",
      "propertyColumns": [
        {
          "name": "time",
          "format": "r"
        },
        {
          "name": "Editable col",
          "format": "r"
        }
      ]
    },
    {
      "id": "table2",
      "type": "table",
      "dataSet": "table-dataset",
      "title": null,
      "addNewRows": true,
      "showBlankRow": false,
      "indexColumn": true,
      "height": "10em",
      "propertyColumns": [
        {
          "name": "time",
          "format": "r",
          "editable": false
        },
        {
          "name": "Editable col",
          "format": "r",
          "editable": false
        }
      ]
    },
    {
      "type": "text",
      "id": "label2",
      "text": "Table use the same data set as graph, so they are synchronized:"
    },
    {
      "type": "graph",
      "id": "graph",
      "dataSet": "graph-dataset",
      "ymin": -100,
      "ymax": 100,
      "xmax": 10,
      "xTickCount": 5,
      "yTickCount": 6,
      "xFormatter": ".2r",
      "yFormatter": ".2s",
      "properties": [
        "totalEnergy"
      ],
      "width": "12em",
      "height": "15em"
    },
    {
      "id": "table3",
      "type": "table",
      "dataSet": "graph-dataset",
      "title": null,
      "addNewRows": false,
      "showBlankRow": false,
      "indexColumn": true,
      "height": "15em",
      "propertyColumns": [
        "displayTime",
        "totalEnergy"
      ]
    }
  ],
  "layout": {
    "bottom": [
      ["label1"],
      ["table1", "table2"],
      ["label2"],
      ["graph", "table3"]
    ]
  }
}