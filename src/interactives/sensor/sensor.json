{
  "title": "Sensor",
  "publicationStatus": "draft",
  "subtitle": "Testing the Sensor model type",
  "about": "",
  "models": [
    {
      "type": "sensor",
      "id": "signal1",
      "url": "models/sensor/sensor.json",
      "viewOptions": {
        "controlButtons": "play_reset",
        "controlButtonStyle": "text",
        "showClock": true
      }
    }
  ],
  "components": [
    {
      "type": "graph",
      "id": "propertiesGraph",
      "title": "Sensor Reading",
      "xlabel": "Time (s)",
      "ymin": 0,
      "ymax": 6,
      "xmax": 30,
      "xTickCount": 6,
      "xFormatter": ".2s",
      "yFormatter": ".2s",
      "properties": [
        "sensorReading"
      ]
    }
  ],
  "layout": {
    "below-model": [
      "propertiesGraph"
    ]
  },
  "exports": {
    "perRun": ["sensorType"],
    "perTick": ["sensorReading"]
  },
  "template": [
    {
      "id": "below-model",
      "top": "model.bottom",
      "width": "model.width",
      "height": "0.7 * model.width"
    },
    {
      "id": "container-bottom",
      "top": "below-model.bottom",
      "width": "model.width"
    }
  ]
}