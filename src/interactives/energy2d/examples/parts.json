{
  "title": "Parts",
  "publicationStatus": "public",
  "subtitle": "Use sliders to change properties of the heater.",
  "models": [
    {
      "type": "energy2d",
      "id": "benard-cell",
      "url": "imports/energy2d/models-json/benard-cell.json",
      "importedFrom": "imports/energy2d/content/benard-cell.e2d",
      "viewOptions": {
        "controlButtons": "play_reset"
      }
    }
  ],
  "parameters": [
    {
      "name": "part-y",
      "label": "Heater Position",
      "unitType": "length",
      "onChange": "getPart(2).y = value;",
      "initialValue": 6.8
    },
    {
      "name": "part-temp",
      "label": "Heater Temperature",
      "unitAbbreviation": "C",
      "onChange": "getPart(2).temperature = value;",
      "initialValue": 20
    }
  ],
  "components": [
    {
      "type": "slider",
      "id": "part-y",
      "title": "Heater Position",
      "property": "part-y",
      "min": 4.4,
      "max": 6.8,
      "displayValue": "return format('1.1f')(value)",
      "steps": 20
    },
    {
      "type": "slider",
      "id": "part-temp",
      "title": "Heater Temperature",
      "property": "part-temp",
      "min": 0,
      "max": 40,
      "displayValue": "return format('1.1f')(value)",
      "steps": 80
    }
  ],
  "layout": {
  }
}